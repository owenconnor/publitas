
<!DOCTYPE html>
<html>
  <head>
    <title>PublitasIntegration</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", media: "all", "data-turbo-track": "reload" %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'topMessageListener', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'shoppingCart', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
    <%= render "layouts/nav_bar" %>
    <%= yield %>
  </body>
</html>
